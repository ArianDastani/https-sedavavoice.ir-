    <div class="resume-wrap">
        <div class="wrap-title">
            <div class="note">
                <span class="icon-user icon"></span>
                <h1 style="font-size: 14px;">داریوش ایل بیگی&nbsp;!&nbsp;</h1>

                <p>سوابق تحصیلی</p>
            </div>
            <div class="box-title">
                <h1 style="font-size:48px;" class="title">
                    تحصیلات<span>!</span>
                </h1>

            </div>



        </div>
        <div class="row">
            @foreach (var item in Model.EducationForindex)
            {
                <div class="col-lg-12">
                    <div class="tf-box-resume animate-down">
                        <ul class="inner-left">
                            <li>@item.EducationPlace</li>
                            <li>@CalenderConvertor.ToShamsi(item.EndDate)</li>
                        </ul>
                        <h4 class="inner-right"><a href="/">@item.Title</a></h4>
                    </div>
                </div>

            }


        </div>

    </div>



