
@using Common.Calender
@model IndexViewModel;


<!-- about us -->
<div class="wedo-section animated fadeInUp active" id="about">
    <div class="about-wrap">
        <div class="top-content">
            <div class="wrap-title st-about">
                <div>
                    <div class="note">
                        <span class="icon-user icon"></span>
                        <h1 style="font-size: 14px;">داریوش ایل بیگی&nbsp;!&nbsp;</h1>

                        <p>درباره من</p>
                    </div>
                    <h1 class="title">
                        درباره من <span>!</span>
                    </h1>
                </div>
                <div class="box-title">
                    <div class="circle-box">
                        <img src="images/home/circle.svg" class="circle" alt="داریوش ایل بیگی">
                        <img style="height: 87.5px; width: 87.5px;" class="logo" src="images/logo-2.png" alt="داریوش ایل بیگی">
                    </div>
                </div>


            </div>
            <p style="text-align:justify;" class="mb15">

                @if (Model.GetInfo.AboutMe != null || Model.GetInfo.AboutMe != " " || Model.GetInfo.AboutMe != "")
                {
                    @Html.Raw(Model.GetInfo.AboutMe)
                    ;
                }
            </p>

        </div>
        <div class="content">

            <div class="inner-top">
                <div class="content-left br-left">
                    <h4>نام</h4>
                    <p>
                        @if (Model.GetInfo.FirstName != null || Model.GetInfo.FirstName != " " || Model.GetInfo.LastName != "")
                        {
                            @Model.GetInfo.FirstName

                        }
                    </p>
                </div>
                <div class="content-right">
                    <h4>تلفن</h4>
                    <p style="direction: ltr;">
                        @if (Model.GetInfo.PhoneNumber != null || Model.GetInfo.PhoneNumber != " " || Model.GetInfo.PhoneNumber != "")
                        {
                            @Model.GetInfo.PhoneNumber

                        }
                    </p>
                </div>
            </div>
            <div class="inner-bottom">
                <div class="content-left br-left br-bottom">
                    <h4>آدرس</h4>
                    <p>
                        @if (Model.GetInfo.Address != null || Model.GetInfo.Address != " " || Model.GetInfo.Address != "")
                        {
                            @Model.GetInfo.Address

                        }
                    </p>
                </div>
                <div class="content-right br-bottom">
                    <h4>تجربه</h4>
                    <p>
                        @if (Model.GetInfo.Experience != null || Model.GetInfo.Experience != " " || Model.GetInfo.Experience != "")
                        {
                            @Model.GetInfo.Experience

                        }
                    </p>
                </div>
            </div>
        </div>


    </div>


    <div class="top-content">
        <div class="wrap-title">
            <div>
                <div class="note">
                    <span class="icon-icon-6 icon"></span>
                    <h1 style="font-size: 14px;">داریوش ایل بیگی&nbsp;!&nbsp;</h1>

                    <p>مهارت های من</p>
                </div>
                <h1 class="title" style="font-size:32px;">
                    مهارت هایی که تا الان دارم و بازاری برای مهارت هایم<span>.</span>
                </h1>
            </div>
            <div class="box-title">
            </div>


        </div>
        <p style="text-align:justify;">
            هرکسی برای پیشرفت کسب و کار خودش
            در هر زمینه ای نیاز به تبلیغات داره و این تبلیغات باید به صورتی باشه که مخاطب هدف و بیننده جذب کسب و کار شما بشه و یکی از اصلی ترین و تاثیر گذار ترین موارد در تبلیغات صدا است که باعث جذب بهتر مخاطب به تبلیغ شما میشه و اینجاست که صداوا گویندگی پروژه ها و ویدیو های شما رو بر عهده میگیره و با صداگذاری روی کارهای شما اون کارو جذاب تر میکنه
        </p>
    </div>

    <div class="tf-skill-bar tf-progress-bar">

        @foreach (var item in Model.SkillsForIndex)
        {

            <div class="progress-item ">
                <h4>@item.Title</h4>
                <div class="inner-skill">
                    <div class="progress-animate" data-valuemax="100" data-valuemin="0" data-valuenow="@item.Percent" style="width: 80%;"></div>
                </div>
            </div>
        }



